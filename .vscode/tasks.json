{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build Dashboard",
      "command": "dotnet",
      "type": "shell",
      "args": ["build"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": "$msCompile"
    },
    {
      "label": "Run Dashboard",
      "command": "dotnet",
      "type": "shell",
      "args": ["run"],
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "dependsOn": "Build Dashboard"
    }
  ]
}
